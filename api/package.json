  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.22.0",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-fastify": "^7.6.15",
    "@nestjs/terminus": "^7.2.0",
    "@skypress/nestjs-dynamodb": "^0.1.0",
    "apollo-server-core": "^2.23.0",
    "apollo-server-fastify": "^3.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fastify": "^3.19.2",
    "graphql": "^15.5.1",
    "graphql-relay": "^0.8.0",
    "graphql-tools": "^7.0.4",
    "jwks-rsa": "^2.0.4",
    "nanoid": "^3.1.22",
    "nestjs-graphql-dataloader": "^0.1.28",
    "nestjs-graphql-relay": "^1.0.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.11",
    "@types/graphql-relay": "^0.7.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
